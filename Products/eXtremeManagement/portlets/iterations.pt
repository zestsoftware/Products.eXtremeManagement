<dl class="portlet portletProjectPortlet"
    i18n:domain="eXtremeManagement">
  <dt class="portletHeader">
    <span class="portletTopLeft"></span>
    <span class="management_portletHeader"
          i18n:translate="head_iterations">Iterations</span>
    <span class="portletTopRight"></span>
  </dt>

  <dd class="portletItem Even">
    <a tal:attributes="href view/project_url"
       i18n:translate="label_current_iteration">Current</a>
  </dd>

  <dd class="portletItem Odd">
    <dl id="iterations-menu" class="actionMenu deactivated"
        tal:condition="view/iterations"
        tal:repeat="link view/iterations">
      <dt class="actionMenuHeader label-iterations">
        <a id="" href="#">
          <span i18n:domain="xm.theme"
                i18n:translate=""
                tal:content="link/title"> State </span>
          <span class="arrowDownAlternative">&#9660;</span>
        </a>
      </dt>
      <dd class="actionMenuContent iterations-submenu">
        <ul>
          <li tal:repeat="iteration link/iterations">
            <a href=""
               tal:attributes="href iteration/url"
               tal:content="iteration/title">Title</a>
          </li>
        </ul>
      </dd>
    </dl>
  </dd>

  <dd class="portletFooter">
    <span class="myProject"></span>
    <span class="portletBottomLeft"></span>
    <span class="portletBottomRight"></span>
  </dd>
</dl>
